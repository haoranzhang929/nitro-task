{
  "name": "nitro-task-be",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server",
    "client": "npm start --prefix client",
    "server": "npm run lint:server && nodemon server/index.ts",
    "dev": "npm run typecheck && concurrently \"npm run server\" \"npm run client\"",
    "build": "tsc --project ./",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "echo 'Type Checking...' && tsc --noEmit && echo 'Type Checking Completed'",
    "lint:server": "echo 'Server Code Lint Checking...' && eslint --ext .js,.jsx,.ts,.tsx server && echo 'Lint Checking Completed'",
    "iall": "npm i && npm i --prefix client"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "author": "Haoran Zhang",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "concurrently": "^5.3.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
